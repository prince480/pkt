    <style>

        body {
            background-image: url('http://10.1.10.26:952/ServerMonitor/input/Files/PKTbg.svg');
            background-size: cover; /* Adjusts the size to cover the whole background */
            }

    </style>


</head>

<body>
    <header>
   
     


    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
