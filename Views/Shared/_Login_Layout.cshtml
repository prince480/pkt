        <div class="container">
            Product Knowledge Test  - &copy; 2023 -  <a asp-area="" asp-controller="Home" asp-action="Privacy">iMarque Solutions Pvt Ltd</a>    
        </div>
    </footer>


    <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.5.0/dist/semantic.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.ui.dropdown').dropdown();
        });
    </script>

 
    <script src="~/js/js-snackbar.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)


    <script>
        var snackBar = function (status, message, icon, timeout, speed, position) {
            SnackBar({
                status: status,   //Default,Success,Warning,Danger,Info//
                message: message,
                icon: icon,        //Exclamation,Question,Plus/ We Can Add any Character//
                timeout: 5000,       // ms //
                speed: speed,       //speed: 500//
                position: "Bottom-Right",       // tl(Top-Left),tc or tm (Top-Center), bl (Bottom-Left),bc or bm (Bottom-Center),br (Bottom-Right)//
                fixed: true,
                //actions: [
                //                    {
                //                        text: "Click Me!",
                //                        function: function () {
                //                            alert("A-C-T-I-O-N");
                //                        }
                //                    }
                //]
            })
        }
    </script>



</body>
</html>
